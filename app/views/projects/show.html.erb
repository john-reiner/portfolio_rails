<p id="notice"><%= notice %></p>

<p>
  <strong>Image:</strong>
  <%= @project.image %>
</p>

<p>
  <strong>Url:</strong>
  <%= @project.url %>
</p>

<p>
  <strong>Github:</strong>
  <%= @project.github %>
</p>

<p>
  <strong>Summary:</strong>
  <%= @project.summary %>
</p>

<p>
  <strong>Created By:</strong>
  <%= @project.user.first_name + " " + @project.user.last_name%>
</p>

<% if @project.deployed %>
  <p>
    <strong>Deployed</strong>
  </p>
<% end %>
<br><br>

<h2>Technologies</h2>

<% @project.technologies.each do |technology| %>
  <p><%= link_to  technology.name, technology_path(technology) %></p>
<% end %>
<br><br>

<hr> <br><br>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', user_path(@current_user.id) %>
