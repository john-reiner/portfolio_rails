<%= form_for :session, url: login_path do |f| %>

    <div class="form-group">
        <%= f.label :email, "Email" %>
        <%= f.text_field :email  %>    
    </div>

    <div class="form-group">
        <%= f.label :password  %>
        <%= f.password_field :password %>
    </div>

    <div>
        <%= f.submit 'Login' %>
    </div>

<% end %>
